[Search]
Type=S
Default		= Window(size=320x540)
Name		= _"Search with tabs [Default]"
Title		= _"Search"
TBmain		= _"Quick Search" VBQuick_Search _"Quick Search with SongTree" VBQuick_Search_with_SongTree _"Artist" ArtistSearch(buttons=1) _"Album" AlbumSearch(buttons=1) _"Song" SongSearch(activate=play,buttons=1)
	VBQuick_Search	= SimpleSearch(activate=SetFocusOn(SongList)) _SongList(activate=play&CloseWindow)
	VBQuick_Search_with_SongTree	= SimpleSearch2(activate=SetFocusOn(SongTree)) _SongTree(activate=play&CloseWindow)
DefaultFocus	= TBmain
KeyBindings	= Escape CloseWindow

